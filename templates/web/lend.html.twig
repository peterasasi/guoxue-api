{# templates/web/card.html.twig #}
{% extends 'base.html.twig' %}
{% block title %}办卡{% endblock %}
{% block stylesheets %}
<style type="text/css">
    .main {
        width: 100%;
        padding: 0rem;
        background: #F3F3F3;
        height: 640px;
        height: calc(100vh);
    }
    .banner-item {
        position: relative;
        margin-bottom: 0.6rem;
    }

</style>
{% endblock %}

{% block body %}
    <div class="main">
    {% for vo in banners  %}
        <div class="banner-item">
            <a href="{{ vo.jump_url }}"><img src="{{ vo.img_url }}" alt="image" class="img-fluid" /></a>
        </div>
    {% endfor %}
    </div>
{% endblock %}

{% block javascripts %}

{% endblock %}
